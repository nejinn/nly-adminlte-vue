		"highlight.js": "^9.18.1",
		"html-loader": "^1.1.0",
		"lodash": "^4.17.15",
		"marked": "^0.8.2",
		"node-sass": "^4.14.1",
		"nuxt": "^2.12.2",
		"prettier": "^1.19.1",
		"sass-loader": "^8.0.2",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended",
			"@vue/prettier"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {},
		"overrides": [
			{
				"files": [
					"**/__tests__/*.{j,t}s?(x)",
					"**/tests/unit/**/*.spec.{j,t}s?(x)"
				],
				"env": {
					"jest": true
				}
			}
		]
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"jest": {
		"preset": "@vue/cli-plugin-unit-jest"
	},
	"vetur": {
		"tags": "dist/vetur-tags.json",
		"attributes": "dist/vetur-attributes.json"
	},
	"files": [
		"packages",
		"dist"
	]
