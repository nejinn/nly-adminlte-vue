        "props": [
          {
            "prop": "buttonClass",
            "version": "0.4.4",
            "description": "按钮元素自定义css"
          }
        ],
        "events": [
          {
            "event": "click",
            "description": "点击emit事件",
            "args": [
              {
                "name": "event",
                "type": "鼠标事件",
                "description": "原生事件"
              }
            ]
          }
        ]
