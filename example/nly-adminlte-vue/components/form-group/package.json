{
  "name": "@nly-adminlte-vue/form-group",
  "version": "1.0.0",
  "meta": {
    "title": "Form Group",
    "subtitle": "表单组",
    "description": "表单组是一种使得标签和表单快速配对的组件",
    "components": [
      {
        "component": "NlyFormGroup",
        "props": [
          {
            "prop": "valid",
            "description": "强制表单验证提示 form-feedback,可选valid, invalid, warning, novalid"
          },
          {
            "prop": "labelSize",
            "description": "大小,可选 sm, lg"
          },
          {
